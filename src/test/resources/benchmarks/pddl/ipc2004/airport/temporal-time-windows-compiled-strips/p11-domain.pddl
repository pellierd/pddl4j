                seg_N4_N5_4_0_85
                seg_N4_N5_5_0_86
                seg_N4_N5_6_0_86 - segment

                airplane_CFBEG
                airplane_HOLGI - airplane
)

(:predicates
                ;; airport information
                (has-type ?a - airplane ?t - airplanetype)
                ;; plane a has type t
                (at-segment ?a - airplane ?s - segment)
                ;; planes are at segments, ie at their end in driving direction
                (facing ?a - airplane ?d - direction)
                ;; planes face into their driving direction

                ;; how the planes affect the airport
                (occupied ?s - segment)
                ;; a plane is in here
                (not_occupied ?s - segment)
                (blocked ?s - segment ?a - airplane)
                ;; segment s is blocked if it is endangered by plane p
                (not_blocked ?s - segment ?a - airplane)

                ;; an airplane may lineup on segment s when facing d
                (is-start-runway ?s - segment ?d - direction)

                ;; airplane a is starting from runway s
                (airborne ?a - airplane ?s - segment)

                (is-moving ?a - airplane)
                (is-pushing ?a - airplane)
                (is-parked ?a - airplane ?s - segment)

                (idled_0)
                (landing_global)
                (landing_started)
                (start_landing)
                (landed_1)
                (idled_1)
                (landed_2)
                (idled_2)
                (landed_last)
                (idled_3)
)

(:functions
                ;; the length of a segment
                (length ?s - segment)

                ;; the number of engines of an airplane
                (engines ?a - airplane)
)

(:durative-action move_seg_A01_0_100_seg_C3_C_A01_0_100_north_north_medium

 :parameters    (?a - airplane)

 :duration
             ;; length of the segment divided through the speed of an airplane (fixed 30 m/s)
             (= ?duration 3)

 :condition  (and
                 (over all (has-type ?a medium))
                 (over all (is-moving ?a))
                 (at start (facing ?a north))
                 (at start (at-segment ?a seg_A01_0_100))
                 (at start (not_occupied seg_C3_C_A01_0_100))
                 (at start (landing_started))
                 (at start (not_blocked seg_C3_C_A01_0_100 airplane_CFBEG))
                 (at start (not_blocked seg_C3_C_A01_0_100 airplane_HOLGI))

              )

 :effect        (and
                 (at end (not (occupied seg_A01_0_100)))
                 (at end (not_occupied seg_A01_0_100))
                 (at end (not (at-segment ?a seg_A01_0_100)))

                 (at start (occupied seg_C3_C_A01_0_100))
                 (at start (not (not_occupied seg_C3_C_A01_0_100)))
                 (at start (blocked seg_C3_C_A01_0_100 ?a))
                 (at start (not (not_blocked seg_C3_C_A01_0_100 ?a)))
                 (at end (at-segment ?a seg_C3_C_A01_0_100))


                )
)

